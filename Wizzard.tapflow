      <action id="1711" type="Custom.Code" displayName="C# Script" code="using System;&#xD;&#xA;using System.Windows;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;using System.Linq;&#xD;&#xA;using System.Text;&#xD;&#xA;using System.Threading;&#xD;&#xA;using System.Net.Mail;&#xD;&#xA;using System.Drawing;&#xD;&#xA;using Tailent.Database;&#xD;&#xA;using System.Data;&#xD;&#xA;using HtmlAgilityPack;&#xD;&#xA;using Newtonsoft.Json.Linq;&#xD;&#xA;using MissionCritical.Automation.Excel;&#xD;&#xA;namespace MissionCritical.Automation.Scripting&#xD;&#xA;{&#xD;&#xA;    public static class CustomCode&#xD;&#xA;    {&#xD;&#xA;		private static string NormalizeExtension(string ext)&#xD;&#xA;{&#xD;&#xA;	    // Add leading dot if missing&#xD;&#xA;	    if (!ext.StartsWith(&quot;.&quot;) &amp;&amp; !string.IsNullOrWhiteSpace(ext))&#xD;&#xA;	    {&#xD;&#xA;	        ext = &quot;.&quot; + ext;&#xD;&#xA;	    }&#xD;&#xA;	    return ext.ToLower(); // Optional: make case insensitive&#xD;&#xA;}&#xD;&#xA;        public static void MainMethod()&#xD;&#xA;        {&#xD;&#xA;            List&lt;string&gt; addedExtensions = new List&lt;string&gt;();&#xD;&#xA;            List&lt;string&gt; removedExtensions = new List&lt;string&gt;();&#xD;&#xA;&#xD;&#xA;            if (!string.IsNullOrEmpty(proceed))&#xD;&#xA;            {&#xD;&#xA;                var commands = proceed.Split(';')&#xD;&#xA;                                    .Select(cmd =&gt; cmd.Trim())&#xD;&#xA;                                    .Where(cmd =&gt; !string.IsNullOrWhiteSpace(cmd));&#xD;&#xA;&#xD;&#xA;               foreach (var cmd in commands)&#xD;&#xA;{&#xD;&#xA;				    bool isAdd = cmd.StartsWith(&quot;+&quot;);&#xD;&#xA;				    bool isRemove = cmd.StartsWith(&quot;-&quot;);&#xD;&#xA;				    &#xD;&#xA;				    if (!isAdd &amp;&amp; !isRemove) continue;&#xD;&#xA;				&#xD;&#xA;				    string ext = NormalizeExtension(cmd.Substring(1).Trim());&#xD;&#xA;                    &#xD;&#xA;                    if (isAdd)&#xD;&#xA;       